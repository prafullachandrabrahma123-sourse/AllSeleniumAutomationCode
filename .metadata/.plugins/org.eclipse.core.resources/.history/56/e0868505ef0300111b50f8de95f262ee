package CoreJavaTopics;

import java.util.Scanner;

public class PracticeJavaPrograms {

	public static void main(String[] args) {
		
		
		int num =6;
		int first=0;
		int second=1;
		
		for(int i =0; i<=num;i++) {
			System.out.println(first+"");
			int next = second+first;
			first=second;
			second=next;
			
		}
	} 
	}


